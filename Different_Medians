#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int f=1;int l=n;
	    for(int i=1;i<=n;i++){
	        if(i%2==1){
	            cout<<l--<<" ";
	        }else{
	            cout<<f++<<" ";
	        }
	    }
	    cout<<endl;
	}
	return 0;
}
